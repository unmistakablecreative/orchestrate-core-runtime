{
  "commands": {
    "Load OrchestrateOS": {
      "run": [
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "executeTask",
          "params": {
            "tool_name": "system_settings",
            "action": "refresh_orchestrate_runtime",
            "params": {}
          }
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "getSupportedActions"
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "executeTask",
          "params": {
            "tool_name": "file_ops_tool",
            "action": "read_file",
            "params": {
              "key": "read_file",
              "filename": "orchestrate_os_protocol.md"
            }
          }
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "executeTask",
          "params": {
            "tool_name": "json_manager",
            "action": "read_json_file",
            "params": {
              "filename": "orchestrate_app_store.json"
            }
          }
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "executeTask",
          "params": {
            "tool_name": "json_manager",
            "action": "read_json_file",
            "params": {
              "filename": "update_messages.json"
            }
          }
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "executeTask",
          "params": {
            "tool_name": "json_manager",
            "action": "read_json_file",
            "params": {
              "filename": "onboarding_sequence.json"
            }
          }
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "executeTask",
          "params": {
            "tool_name": "json_manager",
            "action": "read_json_file",
            "params": {
              "filename": "orchestrate_intent_routes.json"
            }
          }
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "executeTask",
          "id": "secondbrainData",
          "params": {
            "tool_name": "json_manager",
            "action": "read_json_file",
            "params": {
              "filename": "secondbrain.json"
            }
          }
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "executeTask",
          "params": {
            "tool_name": "json_manager",
            "action": "read_json_file",
            "params": {
              "filename": "unlock_nudges.json"
            }
          }
        }
      ],
      "variable_map": {
        "user_name": {
          "source_id": "secondbrainData",
          "path": ["entries", "user_profile", "full_name"]
        }
      },
      "display": [
        {
          "style": "custom",
          "theme": "dark",
          "layout": "standard",
          "display_instructions": {
            "reference_type": "json",
            "reference_doc": "getSupportedActions",
            "render": "markdown_table",
            "title": "üß∞ Welcome, ${user_name} ‚Äî Your Tools",
            "filter": {
              "action": "__tool__"
            },
            "sort_by": "referral_unlock_cost",
            "sort_order": "ascending",
            "columns": [
              "tool",
              "description",
              "üîí Lock State",
              "referral_unlock_cost"
            ]
          }
        },
        {
          "style": "custom",
          "theme": "dark",
          "layout": "standard",
          "display_instructions": {
            "reference_type": "json",
            "reference_doc": "orchestrate_app_store.json",
            "render": "markdown_table",
            "title": "üõçÔ∏è Orchestrate App Store",
            "sort_by": "referral_unlock_cost",
            "sort_order": "ascending",
            "include_tool_name": true,
            "include_descriptions": true,
            "include_credits": true,
            "include_locks": false,
            "include_actions": false,
            "columns": ["Tool", "Description", "Credits"]
          }
        },
        {
          "style": "custom",
          "theme": "dark",
          "layout": "standard",
          "display_instructions": {
            "reference_type": "json",
            "reference_doc": "update_messages.json",
            "render": "json_panel",
            "title": "üîî System Updates"
          }
        },
        {
          "style": "standard",
          "theme": "dark",
          "display_instructions": {
            "reference_type": "json",
            "reference_doc": "onboarding_sequence.json",
            "render": "guided",
            "title": "üë§ Welcome, ${user_name}"
          }
        }
      ],
      "prompt_behavior": "silent"
    }
  }
}
