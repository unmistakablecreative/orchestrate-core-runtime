{
  "tool_ui_reference_doc": "b8d95f06-d7b8-4ebd-9686-bb1bfb38860a",
  "onboarding_sequence": "onboarding_sequence.json",
  "boot_flags": "boot_flags.json",
  "commands": {
    "Load OrchestrateOS": {
      "run": [
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "getSupportedActions"
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "read_json_entry",
          "params": {
            "filename": "boot_flags.json",
            "entry_key": "onboarding_complete"
          }
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "read_json_file",
          "params": {
            "filename": "onboarding_sequence.json"
          },
          "when": "boot_flags.onboarding_complete != true"
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "add_json_entry",
          "params": {
            "filename": "boot_flags.json",
            "entry_key": "onboarding_complete",
            "entry_data": true
          },
          "when": "boot_flags.onboarding_complete != true"
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "read_json_file",
          "params": {
            "filename": "orchestrate_intent_route.json"
          }
        }
      ],
      "display": {
        "style": "black_box_ui",
        "theme": "dark",
        "layout": "terminal_mode",
        "display_instructions": {
          "reference_type": "tool_table",
          "reference_doc": "b8d95f06-d7b8-4ebd-9686-bb1bfb38860a",
          "dynamic_fields": {
            "status": "system_settings.ndjson",
            "credits": "system_settings.ndjson"
          },
          "render": "markdown_table",
          "sort_by": "tool_name"
        }
      },
      "prompt_behavior": "silent"
    }
  },
  "enforcement": {
    "skip_confirmations_for": [],
    "treat_as_internal_ops": true,
    "halt_on_missing_tools": true
  },
  "trigger_phrases": [
    "Load OrchestrateOS"
  ],
  "intent_routing_config": {
    "reference_file": "orchestrate_intent_route.json",
    "match_fields": ["aliases", "seed_phrases"],
    "fallback_to_embedding": true,
    "halt_on_ambiguity": true
  }
}
